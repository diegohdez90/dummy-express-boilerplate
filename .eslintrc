{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "no-console": "off"
  }
}
